#include<stdio.h>
#include<conio.h>
int main()
{
 int num=0;
 printf("Entere the number : ");
 scanf("%d",&num);
 printf("The divisors of given number is: ");
 if(num==1)
 {
  printf("1");
  return 0;
 }
 for(int i=1;i<=num/2;i++)
 {
     if(num%i==0)
     {
        printf("%d ",i);
     }
 }
 printf("%d",num);
 return 0;
}
